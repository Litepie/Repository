{
    "name": "litepie/repository",
    "description": "A Laravel package implementing the Repository pattern with base repository, interfaces, and artisan commands",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "repository",
        "pattern",
        "eloquent",
        "orm",
        "litepie"
    ],
    "authors": [
        {
            "name": "Litepie Team",
            "email": "info@litepie.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/pagination": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/console": "^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^9.0|^10.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Litepie\\Repository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Litepie\\Repository\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Litepie\\Repository\\RepositoryServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
